# SSC API Configuration
SSC_URL=https://ssc.stage.cyberresstage.com/api/v1
SSC_TOKEN=your_fortify_token_here
SSC_TIMEOUT=30000

# Server Configuration
PORT=3000
NODE_ENV=development

# Cache Configuration
CACHE_TTL=900000          # 15 minutes in milliseconds
CACHE_MAX_SIZE=100        # Maximum number of cache entries

# Refresh Configuration
REFRESH_INTERVAL=900000   # 15 minutes in milliseconds

# Logging
LOG_LEVEL=info            # error, warn, info, debug
LOG_DIR=./logs            # Log files directory
